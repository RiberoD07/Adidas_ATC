/*Creates size picker menus */

$(function(){
    var temp = {};
    
    $.each(brandItems, function(){
        $("<option />").attr("value", this.value).html(this.name).appendTo("#retailer-picker");
        temp[this.value] = this.subitems;
    });
    
    $("#retailer-picker").change(function(){
        var value = $(this).val();
        var menu = $("#size-picker");
        
        menu.empty();
        $.each(temp[value], function(){
            $("<option />").attr("value", this.value).html(this.name).appendTo(menu);
        });
    });
      
});

function onLoad(){
	var confirmButton = document.getElementById("confirmButton");
    var logoutButton = document.getElementById("logoutBotButton");
    var extendButton = document.getElementById("extendButton");
	/*Do something when user clicks Confirm button*/
	confirmButton.addEventListener("click", function() {

		var selectSize = document.getElementById('size-picker');
		var currentSize = selectSize.options[selectSize.selectedIndex].value;

		var selectRetailer = document.getElementById('retailer-picker');
		var currentRetailer = selectRetailer.options[selectRetailer.selectedIndex].value;

        switch(currentRetailer){
            case 'Adidas':
            chrome.storage.local.set({"AdidasSize": currentSize}); //This works to set
            break;
            case 'Nike':
            chrome.storage.local.set({"NikeSize": currentSize}); //This works to set
            break;
            case 'FootlockerEU':
            chrome.storage.local.set({"FootlockerEUSize": currentSize}); //This works to set
            break;
            case 'FootlockerUSA':
            chrome.storage.local.set({"FootlockerUSASize": currentSize}); //This works to set
            break;
            case 'Concepts':
            chrome.storage.local.set({"ConceptsSize": currentSize}); //This works to set
            break;
            case 'CrookedTongues':
            chrome.storage.local.set({"CrookedTonguesSize": currentSize}); //This works to set
            break;
            case 'Size':
            chrome.storage.local.set({"SizeSize": currentSize}); //This works to set
            break;
            case 'Footpatrol':
            chrome.storage.local.set({"FootpatrolSize": currentSize}); //This works to set
            break;
            case 'Offspring':
            chrome.storage.local.set({"OffspringSize": currentSize}); //This works to set
            break;
            case 'JDSports':
            chrome.storage.local.set({"JDSportsSize": currentSize}); //This works to set
            break;
            case 'WellGosh':
            chrome.storage.local.set({"WellGoshSize": currentSize}); //This works to set
            break;
            case 'End':
            chrome.storage.local.set({"EndSize": currentSize}); //This works to set
            break;
            case 'SNS':
            chrome.storage.local.set({"SNSSize": currentSize}); //This works to set
            break;
            case 'Soleheaven':
            chrome.storage.local.set({"SoleheavenSize": currentSize}); //This works to set
            break;
            case 'Hanon':
            chrome.storage.local.set({"HanonSize": currentSize}); //This works to set
            break;
            case 'Kith':
            chrome.storage.local.set({"KithSize": currentSize}); //This works to set
            break;
            case 'Supreme':
            chrome.storage.local.set({"SupremeSize": currentSize}); //This works to set
            break;
            case 'ExtraButter':
            chrome.storage.local.set({"ExtraButterSize": currentSize}); //This works to set
            break;
            case 'Bodega':
            chrome.storage.local.set({"BodegaSize": currentSize}); //This works to set
            break;
            /*case '24Kilates':
            chrome.storage.local.set({"KilatesSize": currentSize}); //This works to set
            break;*/
        }
		chrome.tabs.getSelected(null, function(tab) {
	        chrome.tabs.sendMessage(tab.id, {retailer: currentRetailer, size: currentSize},
	            function(response) {
	            }
	        );
    	});

    	window.close();

	});

    logoutButton.addEventListener("click", function(){ //this is just temp for now. will be deleted.
        chrome.storage.local.clear();
        window.close();
    });

    extendButton.addEventListener("click", function(){ //this is just temp for now. will be deleted.
        //chrome.tabs.create({url: "http://www.snkxch.com/shop/bot"});
	chrome.tabs.create({url: "http://google.com/"});        
	window.close();
    });
}

function checkIfExpired(storageDate){
    var d = new Date();
    if (storageDate < d == true){ return true;}
    else{return false;}
}

document.onreadystatechange = function () {
    if (document.readyState === 'complete') {
        var loginButton = document.getElementById("loginButton");
        var signupButton = document.getElementById("signupButton"); 
        var loginPage = document.getElementById("loginPage");
        var contentPage = document.getElementById("actualContent");       

        chrome.storage.local.get({"userObject":{}, "expirationDate":{}, "loggedIn":{},}, function(data) {
             //This works to get
                    var formattedDate = new Date(data.expirationDate);
                    var dateToInsert = (formattedDate).getTime();
                    dateToInsert = new Date(parseInt(dateToInsert));
                    
                    if (data.userObject && (checkIfExpired(formattedDate) == false) && (formattedDate != "Invalid Date") && (data.loggedIn == "true")){
                        //if retrieved data object and the subscription is still in date.
                        contentPage.style.display = "block";
                        loginPage.style.display = "none";

                        var newParagraph = document.createElement("a");
                        newParagraph.href = "#";
                        newParagraph.innerHTML = '<span class="date" title="'+formattedDate+'" data-livestamp="'+dateToInsert+'"></span>';
                        document.getElementById("dateExpires").appendChild(newParagraph);
                        onLoad();
                    }
                    else if (data.userObject && (checkIfExpired(formattedDate) == true)){
                        //if retrieved an object (i.e. user exists) and the subscription has expired.
                        contentPage.style.display = "none";
                        loginPage.style.display = "block";
                        chrome.tabs.create({url: "http://www.snkxch.com/your-account-has-expired/"});
                        chrome.storage.local.clear();                        
                    }
                    else {

                        contentPage.style.display = "none";
                        loginPage.style.display = "block";

                        loginButton.addEventListener("click", function(){
                            chrome.tabs.create({url: "http://www.snkxch.com/sneaker-bot-login"});
                            window.close();
                        });

                        signupButton.addEventListener("click", function(){
                            chrome.storage.local.clear();
                            chrome.tabs.create({url: "http://www.google.com"});
                            window.close();           
                        });
                    }
        });           
    }
}
